{
  "name": "NodeJS-Puppeteer-Crawler-on-MongoDB",
  "version": "0.2.0",
  "description": "A crawler repository for fun",
  "main": "src/index.js",
  "scripts": {
    "api": "./node_modules/.bin/babel-node ./src/api.js",
    "crawl": "./node_modules/.bin/babel-node ./src/crawl.js --crawl-music --crawl-news",
    "crawl-music:dev": "./node_modules/.bin/babel-node ./src/crawl.js --crawl-music --dev-mode",
    "crawl-music:test": "./node_modules/.bin/babel-node ./src/crawl.js --crawl-music --dev-mode --dont-save-data",
    "crawl-news:dev": "./node_modules/.bin/babel-node ./src/crawl.js --crawl-news --dev-mode",
    "crawl-news:test": "./node_modules/.bin/babel-node ./src/crawl.js --crawl-news --dev-mode --dont-save-data",
    "custom": "./node_modules/.bin/babel-node ./src/custom.js",
    "example": "npm run ./node_modules/.bin/babel-node ./src/targets/example.back.js --dont-save-data",
    "mongo:admin": "node ./node_modules/mongo-express/app.js",
    "start": "npm run crawl && npm run api",
    "test": "echo \"No tests!\"; exit 0 "
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hiyali/node-crawler-on-mongodb"
  },
  "keywords": [
    "youtube",
    "music",
    "crawler",
    "node",
    "mongodb"
  ],
  "author": "Salam Hiyali",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.2",
    "mongo-express": "^0.52.2",
    "mongodb": "^2.2.36",
    "restify": "^4.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/runtime": "^7.4.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "eslint": "^4.18.2",
    "puppeteer": "^1.13.0"
  }
}
